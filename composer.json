{
  "name": "dmkzwo/formautoreply",
  "description": "Adds autoreply functionality to the form generator",
  "keywords": ["contao", "form", "autoreply"],
  "type": "contao-module",
  "homepage": "http://www.dmkzwo.de",
  "license": "LGPL-3.0+",
  "authors":[
    {
      "name":"DMKZWO GmbH",
      "homepage":"http://www.dmkzwo.de",
      "role": "Project Leader"
    },
    {
      "name":"Thomas Schabacher",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "~5.4 || ~7.0",
    "contao/core-bundle": "^3.5.2 || ~4.1",
    "contao-community-alliance/composer-plugin": "~2.4 || ~3.0"
  },
  "autoload": {
    "psr-0": {
      "Dmkzwo\\": "system/modules/formautoreply/library/"
    }
  },
  "extra":{
    "contao": {
      "sources":{
        "system/modules/formautoreply": "system/modules/formautoreply"
      }
    }
  }
}